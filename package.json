{
    "name": "http-inspector",
    "version": "0.0.13",
    "description": "",
    "license": "ISC",
    "author": "",
    "files": [
        "lib",
        "inject.js",
        "README.md"
    ],
    "main": "lib/main.js",
    "types": "lib/src/main.d.ts",
    "scripts": {
        "build": "npm run build:types && npm run build:js && npm run build:schema",
        "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline",
        "build:schema": "mkdir -p lib/schemas && typescript-json-schema --strictNullChecks --required src/SerializedTypes.ts SerializedLoggerEvent > lib/schemas/SerializedLoggerEvent.json",
        "build:types": "tsc --emitDeclarationOnly",
        "example:curlLogger": "npm run build:js && node ./lib/examples/curlLogger.js",
        "example:inject": "npm run build:js && HTTP_INSPECTOR=1 HTTP_INSPECTOR_FORMAT=pretty node -r ./inject ./lib/examples/inject.js",
        "example:inject:monitor": "npm run build:js && HTTP_INSPECTOR=1 HTTP_INSPECTOR_FORMAT=monitor node -r ./inject ./lib/examples/inject.js",
        "start-test-server": "node ./__tests__/startServer.js",
        "test": "jest",
        "type-check": "tsc --noEmit",
        "type-check:watch": "npm run type-check -- --watch",
        "lint": "eslint ."
    },
    "dependencies": {
        "nanoid": "^2.1.0",
        "shell-escape": "^0.2.0",
        "zlib": "^1.0.5"
    },
    "devDependencies": {
        "@babel/cli": "^7.5.5",
        "@babel/core": "^7.5.5",
        "@babel/plugin-proposal-class-properties": "^7.5.5",
        "@babel/plugin-proposal-numeric-separator": "^7.2.0",
        "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
        "@babel/preset-env": "^7.5.5",
        "@babel/preset-typescript": "^7.3.3",
        "@types/jest": "^24.0.15",
        "@types/lodash": "^4.14.165",
        "@types/nanoid": "^2.0.0",
        "@types/request-promise-native": "^1.0.16",
        "@types/shell-escape": "^0.2.0",
        "@types/superagent": "^4.1.3",
        "@typescript-eslint/eslint-plugin": "^4.10.0",
        "@typescript-eslint/parser": "^4.10.0",
        "axios": "^0.19.2",
        "compression": "^1.7.4",
        "eslint": "^7.16.0",
        "express": "^4.17.1",
        "jest": "^24.8.0",
        "lodash": "^4.17.15",
        "request-promise-native": "^1.0.7",
        "sleep-promise": "^8.0.1",
        "superagent": "^5.1.0",
        "tslint": "^5.20.0",
        "typescript": "^3.5.3",
        "typescript-json-schema": "^0.40.0"
    }
}
